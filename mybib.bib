	file = {Full Text PDF:/home/oem/Zotero/storage/FITBTVI6/Fiedler and Eloranta - 2010 - Nonadiabatic dynamics by mean-field and surface-ho.pdf:application/pdf;Snapshot:/home/oem/Zotero/storage/AS6VKLW6/00268971003720314.html:text/html}
}

