@article{john_c._tully_nonadiabatic_nodate,
	title = {Nonadiabatic {Dynamics}},
	abstract = {A number of methods have been developed to introduce quantum mechanical effects into an otherwise classical mechanical molecular dynamics framework. Theses methods can be applied to processes involving tunneling and quantuisation of atomic motion on a single potential energy surface. A major challenge for any such mixed quantum-classical approach is feedback; the quantum and classical degrees of freedom must respond to each other properly and self-consistently. This paper examines a number of mixed quantum-classical strategies, includeing mean-field (Ehrenfest) and surface-hopping, with particular attention to the issue of feedback. Each of the methods dicussed has deficiencies, and none displays universal superiority. Directions for further progress are suggested. Nevertheless, the mixed quantum-classical approach has greatly exteneded the range of chemical processes that can be addressed accurately and practically by molecular dynamics simulation.},
	author = {John C. Tully},
	pages = {34--71},
	file = {Tully - 1998 - Nonadiabatic Dynamics.pdf:/home/Sangeya/Zotero/storage/VG4STL6C/Tully - 1998 - Nonadiabatic Dynamics.pdf:application/pdf}
}

