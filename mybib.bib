	pages = {214113},
	file = {Full Text PDF:/home/oem/Zotero/storage/GBBA8KW4/Carof et al. - 2017 - Detailed balance, internal consistency, and energy.pdf:application/pdf;Snapshot:/home/oem/Zotero/storage/ZF4Y6A87/1.html:text/html}
